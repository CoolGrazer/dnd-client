; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DNDThing"
run/main_scene="res://Scenes/Test.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Scripts/Global.gd"

[display]

window/size/viewport_width=240
window/size/viewport_height=160
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[importer_defaults]

font_data_image={
"character_margin": Rect2i(0, 0, 0, 0),
"character_ranges": PackedStringArray("48-58"),
"columns": 18,
"compress": true,
"fallbacks": [],
"image_margin": Rect2i(0, 0, 0, 0),
"rows": 1,
"scaling_mode": 2
}

[physics]

common/max_physics_steps_per_frame=1

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
